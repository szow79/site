<p id="notice"><%= notice %></p>


<% if @project.unique %>
  <%= render "projects/unique/#{@project.name}/#{@project.name}" %>

<% else %>
  <p>
    <strong>Name:</strong>
    <%= @project.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= @project.description %>
  </p>
<% end %>


<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
